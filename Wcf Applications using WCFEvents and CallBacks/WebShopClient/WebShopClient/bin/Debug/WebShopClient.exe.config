                <binding name="NetTcpBinding_IWebshop" />
                <binding name="NetTcpBinding_IBackoffice" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8733/Design_Time_Addresses/webshopservice/Service1/"
